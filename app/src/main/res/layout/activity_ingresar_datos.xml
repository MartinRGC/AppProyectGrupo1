<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".IngresarDatos">

    <Button
        android:id="@+id/button4"
        android:layout_width="304dp"
        android:layout_height="44dp"
        android:background="@drawable/rectsignup"
        android:text="Continuar"
        android:textAppearance="@style/login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText8" />

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="317dp"
        android:layout_height="51dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:inputType="text"
        android:text="Numero"
        android:textAppearance="@style/email"
        android:textColor="#949494"
        app:layout_constraintTop_toBottomOf="@+id/editTextText7"
        tools:layout_editor_absoluteX="49dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="317dp"
        android:layout_height="51dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:inputType="text"
        android:text="Nombre"
        android:textAppearance="@style/email"
        android:textColor="#949494"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:layout_editor_absoluteX="49dp" />

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="317dp"
        android:layout_height="51dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:inputType="text"
        android:text="Apellido"
        android:textAppearance="@style/email"
        android:textColor="#949494"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:layout_editor_absoluteX="49dp" />

    <EditText
        android:id="@+id/editTextText7"
        android:layout_width="317dp"
        android:layout_height="51dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:inputType="text"
        android:text="Correo"
        android:textAppearance="@style/email"
        android:textColor="#949494"
        app:layout_constraintTop_toBottomOf="@+id/editTextText6"
        tools:layout_editor_absoluteX="49dp" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="41dp"
        android:layout_height="61dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/felcha"
        tools:layout_editor_absoluteX="49dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Ingrese sus datos para continuar"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        tools:layout_editor_absoluteX="51dp" />

</androidx.constraintlayout.widget.ConstraintLayout>